{
  "problemMatcher": [
    {
      "owner": "python-pylint",
      "pattern": [
        {
          "regexp": "^(.*?):(\\d+):(\\d+): ([A-Z])(\\d{4}): (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "code": 5,
          "message": 6
        }
      ]
    }
  ]
}